{
  "name": "@ibm-ics/resource-base-service",
  "version": "0.0.1",
  "license": "ICS",
  "main": "dist8/src/server.js",
  "scripts": {
    "pretest": "npm run build",
    "start": "cross-env REDIS_INSTALLED=false NODE_ENV=development nodemon --watch \"src/**/*.ts\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node\" src/server.ts",
    "start-prod": "REDIS_INSTALLED=false NODE_ENV=${NODE_ENV:-'production'} node dist8/src/server.js",
    "test": "npm run test:unit && npm run lint",
    "test:unit": "TEST_SUITE=unit nyc node tests/jasmine-runner.ts",
    "test:api": "ts-node ./tests/api/index.ts",
    "posttest": "npm prune --production",
    "lint": "tslint --project tsconfig.json \"{src,tests}/**/*.ts\" -t stylish && npm run format:dry-run",
    "format:dry-run": "prettier --config .prettierrc --list-different \"{src,tests}/**/*.{ts,json}\"",
    "format": "prettier --config .prettierrc --write \"{src,tests}/**/*.{ts,json}\"",
    "build": "ics-clean ./dist8/ && ics-tsc",
    "artifactory": "node ./tools/artifactory.js"
  },
  "devDependencies": {
    "@ibm-ics/build": "0.0.5",
    "@ibm-ics/common-lib-test-node": "0.0.1",
    "@ibm-ics/nyc-config-ics": "0.0.7",
    "@ibm-ics/tslint-config-ics": "0.0.7",
    "@types/express": "4.11.1",
    "@types/jasmine": "2.8.8",
    "@types/node": "10.0.8",
    "request": "2.87.0",
    "cpx": "1.5.0",
    "cross-env": "5.1.6",
    "loopback-connector-cassandra": "1.3.0",
    "loopback-connector-ibmcassandra": "0.0.10",
    "jasmine": "3.1.0",
    "jasmine-reporters": "2.3.1",
    "jasmine-spec-reporter": "4.2.1",
    "nodemon": "1.12.0",
    "nyc": "11.9.0",
    "prettier": "1.13.5",
    "reqres": "2.1.0",
    "ts-node": "6.1.1",
    "tslint": "5.10.0",
    "typescript": "2.9.1"
  },
  "dependencies": {
    "@newrelic/native-metrics": "3.0.0",
    "@types/bluebird": "3.5.21",
    "bluebird": "3.5.1",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "helmet": "3.12.0",
    "lodash": "4.17.10",
    "loopback": "3.20.0",
    "loopback-boot": "3.1.0",
    "loopback-component-explorer": "6.0.1",
    "loopback-connector-cassandra": "1.5.0",
    "loopback-connector-mongodb": "3.4.4",
    "newrelic": "4.1.5",
    "rorschach": "^0.7.2",
    "serve-favicon": "2.5.0",
    "strong-error-handler": "3.0.0",
    "uuid": "3.2.1",
    "xss": "0.3.8",
    "zookeeper": "^3.4.9-3"
  },
  "repository": {
    "type": "",
    "url": ""
  }
}
